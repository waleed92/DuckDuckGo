    private async Task<IEnumerable<string>> Search1(string value)
    {
        if (value != null)
        {
            var results = new List<String>();
            var result = await GetLocations(value);

            // if text is null or empty, show complete list

            if (result != null)
            {
                foreach (var autoCompleteResult in result.results)
                {
                    if (!string.IsNullOrEmpty(autoCompleteResult.content))
                    {
                        var content = autoCompleteResult.content.Split(',');
                        foreach (var location in content)
                        {
                            results.Add(location.Replace("\"", ""));
                        }
                    }
                }
            }

            return results;

        }

        return new List<String>();
    }

    private async Task<SearchResult> GetLocations(string searchText)
    {
        var result = await httpClient.GetFromJsonAsync<SearchResult>("api/Location/?searchText=" + searchText);
        return result ?? new SearchResult();
    }

    public class AutoCompleteResult
